#ifndef _LOADER_HPP_
#define _LOADER_HPP_

namespace orion
{

template <class ContainerType>
class Loader
{
public:
	Loader() {}
	virtual ~Loader() {}

	virtual bool load( ContainerType& container ) = 0;
};

} // namespace orion
#endif // _LOADER_HPP_
